{
  "name": "gamehub",
  "version": "0.0.1",
  "private": true,
  "rnpm": {
    "assets": [
      "./src/Assets/fonts"
    ]
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint --ext .js App",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test": "jest",
    "android:emulator": "cd $ANDROID_HOME/tools && ./emulator -avd react-native",
    "precommit": "npm run lint -s",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android"
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.0",
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "babel-register": "^6.26.0",
    "client": "0.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "formik": "^1.4.1",
    "jest": "^23.6.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.5.0",
    "react-native": "0.56.0",
    "react-native-i18n": "^2.0.15",
    "react-native-network-info": "^4.0.1",
    "react-native-svg": "^6.4.1",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.16.0",
    "react-navigation-backhandler": "^1.1.1",
    "react-redux": "^5.0.7",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "socket.io-client": "^2.2.0",
    "styled-components": "^3.4.10",
    "yup": "^0.24.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0-beta.47",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "^5.0.2",
    "babel-runtime": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.11.1",
    "firebase-mock": "^2.2.10",
    "jest-expo": "26.0.0",
    "jsdom": "^16.5.0",
    "jsdom-global": "^3.0.2",
    "mockdate": "^2.0.2",
    "react-native-mock-render": "^0.1.2",
    "react-native-scripts": "1.13.1",
    "react-test-renderer": "^16.3.0-alpha.1"
  }
}
